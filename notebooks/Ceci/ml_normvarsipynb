    "unique_loans = df['type_of_loan'].unique()\n",
    "\n",
    "# Create a mapping for 'not specified' values to code 666\n",
    "loan_mapping = {}\n",
    "for loan in unique_loans:\n",
    "    if loan in not_specified_keywords:\n",
    "        loan_mapping[loan] = 666  # Assign code 666 for exact matches\n",
    "    else:\n",
    "        loan_mapping[loan] = random.randint(100, 999)  # Assign a random 3-digit code to other values\n",
    "\n",
    "# Apply the mapping to the 'type_of_loan' column\n",
    "df['type_of_loan_encoded'] = df['type_of_loan'].map(loan_mapping)\n",
    "\n",
    "# Print the encoding table with loan types and their corresponding codes\n",
    "encoding_table = pd.DataFrame({\n",
    "    'Loan Type': loan_mapping.keys(),\n",
    "    'Encoded Value': loan_mapping.values()\n",
    "})\n",
    "\n",
    "# Show the encoding table\n",
    "print(encoding_table)\n",
    "\n",
    "# Save the encoding table to the current directory for future reference\n",
    "#encoding_table.to_csv('loan_type_encoding.csv', index=False)"
   ]
  },
