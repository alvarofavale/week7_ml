    "# Define the mapping for the 'occupation' column\n",
    "occupation_mapping = {\n",
    "    'Scientist': 1,\n",
    "    'Teacher': 2,\n",
    "    'Engineer': 3,\n",
    "    'Entrepreneur': 4,\n",
    "    'Developer': 5,\n",
    "    'Lawyer': 6,\n",
    "    'Media Manager': 7,\n",
    "    'Doctor': 8,\n",
    "    'Journalist': 9,\n",
    "    'Manager': 10,\n",
    "    'Accountant': 11,\n",
    "    'Musician': 12,\n",
    "    'Mechanic': 13,\n",
    "    'Writer': 14,\n",
    "    'Architect': 15\n",
    "}\n",
    "\n",
    "# Apply the mapping to the 'occupation' column\n",
    "df['occupation_encoded'] = df['occupation'].map(occupation_mapping)\n",
    "\n",
    "# Get the counts of each occupation in the encoded form\n",
    "occupation_counts = df['occupation_encoded'].value_counts().sort_index()\n",
    "\n",
    "# Prepare the count table by matching only the occupations present in the dataset\n",
    "occupation_count_table = pd.DataFrame({\n",
    "    'Encoded Value': occupation_counts.index,\n",
    "    'Count': occupation_counts.values\n",
    "})\n",
    "\n",
    "# Merge the original occupation names with their encoded values from the mapping\n",
    "occupation_count_table['Occupation'] = occupation_count_table['Encoded Value'].map({v: k for k, v in occupation_mapping.items()})\n",
    "\n",
    "# Print the table\n",
    "print(occupation_count_table)"
   ]
